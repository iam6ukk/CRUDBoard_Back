<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mappper.BoardMapper">

    <select id="getBoardList" resultType="com.example.demo.domain.Board">

        SELECT seq,
               title,
               content,
               reg_dt
        FROM tb_board;

    </select>

    <insert id="insertBoard">
        insert into tb_board (seq, title, content, reg_dt) values (?,?,?,?);
    </insert>

</mapper>